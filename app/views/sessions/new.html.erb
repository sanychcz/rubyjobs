<div class="row">
	<div class="large-6 large-centered columns">
		<h1>Sign in</h1>
	</div>	
</div>

<div class="row">
	<div class="large-6 large-centered columns">
		<%= form_for( :session, url: sessions_path ) do |f| %>
			<%= f.label :email %>
			<%= f.text_field :email %>

			<%= f.label :password %>
			<%= f.password_field :password %>

			<%= f.submit "Sign in" %>
		<% end %>
	</div>
</div>